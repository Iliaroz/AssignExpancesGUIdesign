<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="frm_bells">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Labelframe" id="lblfr_weather">
        <property name="height">200</property>
        <property name="text" translatable="yes">Weather</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="ipadx">10</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Label" id="var_location">
            <property name="font">{Arial} 16 {}</property>
            <property name="text" translatable="yes">Please, enter the city name above</property>
            <property name="textvariable">string:location</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">4</property>
              <property name="pady">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="cv_icon">
            <property name="background">#5363ee</property>
            <property name="height">50</property>
            <property name="width">50</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_temp_number">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_today">
            <property name="font">{Arial} 12 {}</property>
            <property name="text" translatable="yes">Today is</property>
            <property name="textvariable">string:lab</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">50</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_type">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_temperature">
            <property name="text" translatable="yes">Temperature</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="pygubu.builder.widgets.calendarframe" id="cal_Bells">
            <property name="firstweekday">6</property>
            <property name="month">1</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="columnspan">3</property>
              <property name="padx">50</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="rowspan">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_time">
            <property name="font">{Arial} 12 {}</property>
            <property name="text" translatable="yes">Time is</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">50</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_time">
            <property name="font">{Arial} 12 {}</property>
            <property name="text" translatable="yes">&lt;time&gt;</property>
            <property name="textvariable">string:time</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_lt_date">
            <property name="font">{Arial} 12 {}</property>
            <property name="text" translatable="yes">Selected date:</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">50</property>
              <property name="pady">20</property>
              <property name="propagate">True</property>
              <property name="row">8</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_lt_cal">
            <property name="font">{Arial} 12 {}</property>
            <property name="text" translatable="yes">&lt;Selected date&gt;</property>
            <property name="textvariable">string:selection</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">8</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_todayDate">
            <property name="font">{Arial} 12 {}</property>
            <property name="text" translatable="yes">&lt;todays date&gt;</property>
            <property name="textvariable">string:today</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_wind">
            <property name="text" translatable="yes">Wind</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_wt_Wind">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_pressure">
            <property name="text" translatable="yes">Pressure</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_wt_Pressure">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_wt_tMax">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_max_tempo">
            <property name="text" translatable="yes">Temp.max</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_tmep_min">
            <property name="text" translatable="yes">Temp.min</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_wt_tMin">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label26">
            <property name="text" translatable="yes">Humidity</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">40</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="var_wt_Humidity">
            <property name="text" translatable="yes">UNKNOWN</property>
            <property name="textvariable">string:default</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="lbfr_choose">
        <property name="height">200</property>
        <property name="text" translatable="yes">Choose city for which you want to have information displayed</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="lbl_city">
            <property name="text" translatable="yes">Please, write the city name in the box</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">50</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="ent_city">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="ipadx">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btn_city">
            <property name="text" translatable="yes">Confirm</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">50</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
